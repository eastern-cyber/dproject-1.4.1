{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["dom", "dom.iterable","ES2020", "esnext", "DOM", "DOM.Iterable"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/referrer/[referrerId]/page.tsx.bak", "src/app/bonus/page.tsx.bak3"],
  "exclude": ["node_modules"]
}
