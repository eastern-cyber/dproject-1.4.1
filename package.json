{
  "name": "neon-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build --webpack",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "db:init": "npx tsx scripts/init-db.ts",
    "db:reset": "npx tsx scripts/reset-db.ts",
    "db:import:production": "npx tsx scripts/import-production-users.ts",
    "db:import:prod:force": "DATABASE_URL=\"your-production-connection\" npx tsx scripts/import-production-users.ts",
    "db:recreate:exact": "npx tsx scripts/recreate-exact-table.ts",
    "db:import:exact": "npx tsx scripts/import-exact-production.ts",
    "db:setup:production": "npm run db:recreate:exact && npm run db:import:exact",
    "db:import:corrected": "npx tsx scripts/import-corrected-production.ts",
    "db:setup:corrected": "npm run db:recreate:exact && npm run db:import:corrected",
    "import-dproject-users": "node import-dproject-users.js"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.1",
    "@thirdweb-dev/sdk": "^4.0.99",
    "@walletconnect/logger": "^3.0.0",
    "@walletconnect/sign-client": "^2.23.0",
    "@walletconnect/universal-provider": "^2.23.0",
    "axios": "^1.11.0",
    "chart.js": "^4.5.0",
    "lucide-react": "^0.542.0",
    "next": "^16.0.7",
    "pg": "^8.16.3",
    "pino": "^8.17.0",
    "pino-pretty": "^13.1.1",
    "postgres": "^3.4.7",
    "react": "^19.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.2.0",
    "recharts": "^3.1.2",
    "thirdweb": "^5.115.3",
    "thread-stream": "^2.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "20.19.12",
    "@types/react": "19.2.7",
    "@types/react-dom": "^19.2.3",
    "desm": "^1.3.1",
    "dotenv": "^17.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "fastbench": "^1.0.1",
    "ignore-loader": "^0.1.2",
    "pino-elasticsearch": "^8.1.0",
    "tailwindcss": "^4",
    "tap": "^21.5.0",
    "tape": "^5.9.0",
    "tsx": "^4.20.5",
    "typescript": "^5",
    "why-is-node-running": "^3.2.2"
  },
  "compilerOptions": {
    "paths": {
      "baseUrl": ".",
      "@public/*": [
        "./public/*"
      ],
      "@lib/*": [
        "./src/lib/*"
      ]
    }
  }
}
